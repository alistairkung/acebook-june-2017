<h1>Users#show</h1>

<p><%= @user.id %></p>

<%= link_to "New post", new_post_url(:user_id => @user.id) %>

<% if !@user.posts.empty? %>
<ul>
  <% @user.posts.reverse.each do |post| %>
 <li>  <%= post.message %> </li>
<li>  <%= post.created_at %> </li>
    <span id="post"> <% post.user.id %> </span>
  <% end %>
</ul>
  <% end %>
